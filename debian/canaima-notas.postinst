case "$1" in
    install|configure)
    ;;
	mkdir -p $(CURDIR)/debian/canaima-notas/usr/bin/
	mkdir -p $(CURDIR)/debian/canaima-notas/usr/share/canaima-notas/
	mkdir -p $(CURDIR)/debian/canaima-notas/usr/share/applications/
	mv canaima-notas.py $(CURDIR)/debian/canaima-notas/usr/bin/canaima-notas
	cp canaima-notas.png $(CURDIR)/debian/canaima-notas/usr/share/canaima-notas/
	cp canaima-notas.glade $(CURDIR)/debian/canaima-notas/usr/share/canaima-notas/
	cp canaima-notas.desktop $(CURDIR//debian/canaima-notas/usr/share/applications/

    
    ;;

    *)
    ;;
esac

